{
	"source": {
		"include": ["./components", "./pages"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/",
		"template": "node_modules/better-docs",
		"readme": "./readme.md"
	}
}
